{
  "name": "retrospectives-client",
  "version": "0.0.1",
  "description": "Web client for retrospectiv.es",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf ./build/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "build": "ENVIRONMENT=production node node_modules/webpack/bin/webpack.js -p && cp index.html build/",
    "docker-build": "docker build -t oskanberg/retrospectives-client .",
    "docker-dist": "docker push oskanberg/retrospectives-client",
    "dist": "npm run clean && npm run build && npm run docker-build && npm run docker-dist",
    "dev": "ENVIRONMENT=development node node_modules/webpack/bin/webpack.js -d --watch"
  },
  "author": "Oliver Skanberg-Tippen",
  "license": "UNLICENSED",
  "homepage": "http://retrospectiv.es",
  "dependencies": {
    "express": "^4.13.3"
  },
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "eslint": "^3.11.1",
    "expect": "^1.6.0",
    "install": "^0.8.2",
    "isomorphic-fetch": "^2.2.1",
    "material-design-icons": "^3.0.1",
    "material-ui": "^0.16.5",
    "node-libs-browser": "^0.5.2",
    "npm": "^4.0.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-motion": "^0.4.7",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-swipeable-views": "^0.9.3",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.9.11"
  }
}
